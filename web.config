<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="FastCgiModule" scriptProcessor="D:\Python\Python.exe|D:\Python\Scripts\wfastcgi.py" resourceType="Unspecified" requireAccess="Script"/>
    </handlers>
  </system.webServer>
  
  <appSettings>
    <add key="WSGI_HANDLER" value="config.wsgi.application"/>
    <add key="PYTHONPATH" value="D:\home\site\wwwroot"/>
    <add key="DJANGO_SETTINGS_MODULE" value="config.settings.azure_production"/>
  </appSettings>
</configuration> 